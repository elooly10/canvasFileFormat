clear
width 500
height 500

fill style red
stroke style blue
stroke width 5

begin path
// Draw a filled rectangle
fill rect 50 50 100 100

// Draw a stroked rectangle
stroke rect 200 50 100 100

begin path
stroke style #00cc00
fill style #008000
// Draw some circles
circle 250 250 50
begin path
circle 300 250 50
fill
stroke

begin path
stroke style orange
// Draw some ellipses
ellipse 150 200 80 40 0 1 2
use degrees
ellipse 300 400 80 60 0 100 200
fill
stroke

begin path
stroke style purple
// Draw some arcs
use rad
arc 100 100 50 0 PI
arc 200 100 50 0 1PI true
close path
fill
stroke
